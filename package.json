{
    "name": "argon-dashboard-react",
    "version": "1.0.0",
    "main": "index.js",
    "keywords": [
        "react",
        "reactjs",
        "argon",
        "argon-react",
        "dashboard",
        "dashboard-react",
        "argon-dashboard",
        "argon-dashboard-react"
    ],
    "homepage": "/",
    "scripts": {
        "start": "react-app-env start --env-file=.env",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "postbuild": "node cached-assets.js && react-snap",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
        "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
        "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
        "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@material-ui/core": "^4.9.5",
        "@material-ui/lab": "^4.0.0-alpha.37",
        "@redux-offline/redux-offline": "^2.6.0",
        "@reduxjs/toolkit": "^1.4.0",
        "@sentry/browser": "^5.17.0",
        "axios": "^0.19.0",
        "axios-offline": "^1.5.2",
        "babel-polyfill": "^6.26.0",
        "chart.js": "2.7.3",
        "classnames": "2.2.6",
        "glob": "^7.1.6",
        "localforage": "^1.9.0",
        "material-ui": "^0.20.2",
        "moment": "^2.24.0",
        "nouislider": "13.1.1",
        "pdf-viewer-reactjs": "^2.0.7",
        "react": "16.13.0",
        "react-chartjs-2": "2.7.4",
        "react-collapse": "^5.0.1",
        "react-copy-to-clipboard": "5.0.1",
        "react-datetime": "2.16.3",
        "react-dnd": "^2.6.0",
        "react-dnd-html5-backend": "^3.0.2",
        "react-dom": "16.8.4",
        "react-google-maps": "9.4.5",
        "react-moment": "^0.9.7",
        "react-redux": "^7.2.1",
        "react-router": "^5.2.0",
        "react-router-dom": "5.1.2",
        "react-scripts": "2.1.8",
        "react-select": "^3.0.8",
        "react-tag-input": "^6.2.1",
        "reactstrap": "8.4.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.3.0",
        "replace-in-file": "^6.1.0"
    },
    "devDependencies": {
        "@types/googlemaps": "3.30.18",
        "@types/markerclustererplus": "2.1.33",
        "@types/react": "16.8.7",
        "node-sass": "^4.11.0",
        "prop-types": "^15.7.2",
        "react-app-env": "^1.2.3",
        "react-snap": "^1.23.0",
        "redux-devtools": "^3.6.1",
        "typescript": "3.3.3333"
    }
}
